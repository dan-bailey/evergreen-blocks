{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "evergreen-blocks/step-2-block-controls",
	"version": "1.0.0",
	"title": "Block Controls",
	"category": "evergreen-blocks",
	"icon": "admin-generic",
	"description": "Block customization using Block controls: Block Toolbar(BlockControls) and Settings Sidebar(InspectorControls)",
	"keywords": [ "boilerplate","block controls" ],
